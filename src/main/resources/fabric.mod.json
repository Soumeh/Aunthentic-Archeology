{
    "schemaVersion": 1,
    "version": "${mod_version}",

    "id": "aunthetic_archeology",
    "name": "Aunthetic Archeology",
    "description": "W.I.P.",
    "authors": [ "W.I.P." ],
    "contact": {
        "_homepage": "https://github.com/InsomniacsMods",
        "_sources": "https://github.com/InsomniacsMods/Manic",
        "_issues": "https://github.com/InsomniacsMods/Manic/issues"
    },
    "license": "W.I.P.",

    "entrypoints": {
        "main": [ "net.insomniacs.manic.Manic" ],
        "client": [ "net.insomniacs.manic.ManicClient" ]
    },
    "mixins": [
        "aunthetic_archeology.mixins.json",
        {
            "config": "aunthetic_archeology.client.mixins.json",
            "environment": "client"
        }
    ],
    "accessWidener": "aunthetic_archeology.accesswidener",

    "custom": {
        "loom:injected_interfaces": {}
    },

    "depends": {
        "java": ">=21",
        "minecraft": "${minecraft_version}",
        "fabric-api": ">=${fabric_version}"
    }
}