{
    "schemaVersion": 1,
    "version": "${mod_version}",

    "id": "authentic_archeology",
    "name": "Authentic Archeology",
    "description": "W.I.P.",
    "authors": [ "W.I.P." ],
    "contact": {
        "_homepage": "https://github.com/InsomniacsMods",
        "_sources": "https://github.com/InsomniacsMods/Manic",
        "_issues": "https://github.com/InsomniacsMods/Manic/issues"
    },
    "license": "W.I.P.",

    "entrypoints": {
        "main": [ "com.sindercube.aa.AuthenticArcheology" ],
        "client": [ "com.sindercube.aa.client.AuthenticArcheologyClient" ]
    },
    "mixins": [
        "authentic_archeology.mixins.json",
        {
            "config": "authentic_archeology.client.mixins.json",
            "environment": "client"
        }
    ],
    "accessWidener": "authentic_archeology.accesswidener",

    "custom": {
        "loom:injected_interfaces": {}
    },

    "depends": {
        "java": ">=21",
        "minecraft": "${minecraft_version}",
        "fabric-api": ">=${fabric_version}"
    }
}